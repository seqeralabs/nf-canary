nextflow_process {

    name "Test Process TEST_FAIL"
    script "main.nf"
    process "TEST_FAIL"

    test("TEST_FAIL") {

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process).match() },
                { assert process.trace.failed().size() == 1 },
                { assert process.trace.succeeded().size() == 0 }
            )
        }

    }

}
